.zip
.tar
.gz
.bz2
.7z
.rar

# Binary/Compiled
.exe
.dll
.so
.dylib
.a
.o
.pyc
.class
.jar

# Media
.mp3
.mp4
.avi
.mov
.wav
.flac

# Data
.db
.sqlite
